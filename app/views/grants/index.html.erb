<!--<p id="flashNotice"><%#= notice %></p>-->

<% if @grants.present? %>
	<div id="dashboard-rollup" data-step="2" data-intro="The rollup dashboard provides quick information at a glance, and is updated immediately when new data is uploaded.">
		<%= render 'cardRollup' %>
	</div>
	<div id="chart-area"> 
		<!-- load drill-down chart -->
		<div id="drill-down-chart">
			<%= render 'drillDownChart' %>
		</div>
		
		<!-- load line chart -->
		<div id="line-stack-chart">
			<%= render 'lineChart' %>
		</div>
		<!-- load stacked chart -->
		<div id="stacked-chart">
			<%= render 'stackedChart' %>
		</div>
		
	</div>
	
	<!-- load data table -->
	<%= render 'dataTable' %>	
	
	<!-- faq area -->
	<%= render 'faqArea' %>	

<% else %>	
<% end %>

<!-- uncomment this to run console on the page-->
<%#= console %>